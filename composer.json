{
    "name": "mcfedr/resque-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A bundle for resque",
    "autoload": {
        "psr-4": { "Mcfedr\\ResqueBundle\\": "src/Mcfedr/ResqueBundle/" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\ResqueBundle\\" : "tests/Mcfedr/ResqueBundle/" }
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~2.8|~3.0",
        "mcfedr/php-resque": "~1.3",
        "mcfedr/php-resque-scheduler": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "suggest": {
        "ext-redis": "Improve performance using redis"
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    }
}
